#!/usr/bin/env python
# coding: utf-8
# cortex | A command line client for cortex-server

import sys, os
#sys.path.append(os.path.join(os.path.dirname(os.path.realpath(__file__)), 'lib'))

from cortex_client import dispatch

def main(argv):
    dispatch.run(argv)

if __name__ == "__main__":
    main(sys.argv[1:])
